{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "baseUrl": ".",
    "paths": {
      "@infra/*": [
        "./src/infra/*"
      ],
      "@routes/*": [
        "./src/infra/routes/*"
      ]
    }
  },
  "module": {
    "type": "commonjs"
  }
}